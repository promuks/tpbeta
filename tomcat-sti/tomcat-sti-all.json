{
   "kind": "List",
   "apiVersion": "v1",
   "metadata": {
      "name": "tpbeta-tomcat-sti"
   },
   "items": [
      {
         "kind": "BuildConfig",
         "apiVersion": "v1",
         "metadata": {
            "name": "tpbeta-tomcat-sti"
         },
         "spec": {
            "triggers": [
               {
                  "type": "GitHub",
                  "github": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "Generic",
                  "generic": {
                     "secret": "secret"
                  }
               },
               {
                  "type": "ImageChange",
                  "imageChange": {}
               }
            ],
            "source": {
               "type": "Git",
               "git": {
                  "uri": "https://github.com/promuks/tpbeta.git",
                  "ref": "master"
               },
               "contextDir": "tomcat-sti"
            },
            "strategy": {
               "type": "Docker",
               "dockerStrategy": {
                  "from": {
                     "kind": "ImageStreamTag",
                     "name": "rhel7.2:latest"
                  },
                    "noCache": true
               }
            },
            "output": {
               "to": {
                  "kind": "ImageStreamTag",
                  "name": "tpbeta-tomcat-sti:latest"
               }
            },
            "resources": {}
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "rhel7.2"
         },
         "spec": {
            "dockerImageRepository": "registry.access.redhat.com/rhel7.2:latest"
         }
      },
      {
         "kind": "ImageStream",
         "apiVersion": "v1",
         "metadata": {
            "name": "tpbeta-tomcat-sti"
         },
         "spec": {
            "dockerImageRepository": "",
            "tags": [
               {
                  "name": "latest",
                  "annotations": {
                     "description": "Tomcat STI image",
                     "iconClass": "icon-jboss",
                     "tags": "builder,tomcat,java,maven,angular",
                     "supports": "tomcat,java:8,maven:3,angular:2.6",
                     "version": "1.0"
                  }
               }
            ]
         }
      } 

   ]
}
